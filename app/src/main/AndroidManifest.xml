<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pusat.android.makananbekuenak.com.aplikasi_distributor" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:label="Distributor"
        android:theme="@style/CustomActionBarTheme">

        <activity android:name=".HomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="pusat.android.makananbekuenak.com.aplikasi_distributor.LoginActivity"
            android:theme="@style/AppTheme.Dark"/>
        <activity
            android:name="pusat.android.makananbekuenak.com.aplikasi_distributor.SignupActivity"
            android:theme="@style/AppTheme.Dark"/>
        <activity android:name=".UpdateStok"/>
        <activity
            android:name=".KirimOrder"
            android:label="Kirim Order"
            android:configChanges="orientation|screenSize"
            android:windowSoftInputMode="adjustPan|adjustResize">
            <!--Coding Kembali Ke Menu Utama-->
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="pusat.android.makananbekuenak.com.aplikasi_distributor.HomeScreen"/>
            <!-- END Coding Kembali Ke Menu Utama-->
        </activity>
        <activity
            android:name=".Pengaturan"
            android:label="Pengaturan"
            android:configChanges="orientation|screenSize"
            android:windowSoftInputMode="adjustPan|adjustResize" >
            <!--Coding Kembali Ke Menu Utama-->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pusat.android.makananbekuenak.com.aplikasi_distributor.HomeScreen"/>
            <!-- END Coding Kembali Ke Menu Utama-->
        </activity>
        <activity
            android:name=".Laporan"
            android:label="Laporan"
            android:configChanges="orientation|screenSize"
            android:windowSoftInputMode="adjustPan|adjustResize" >
            <!--Coding Kembali Ke Menu Utama-->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pusat.android.makananbekuenak.com.aplikasi_distributor.HomeScreen" />
            <!-- END Coding Kembali Ke Menu Utama-->
        </activity>
        <activity android:name=".EditdataPribadi"/>
        <activity android:name=".RegistrasiDistributor"/>
    </application>
</manifest>